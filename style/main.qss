/*make the qss(css) scrollBar*/
* {
    font-family: system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
}
QScrollBar 
{
    background:transparent;
    padding-top:6px;
    padding-bottom:6px;
}
/**/
QScrollBar:vertical 
{
    width: 10px;
}
QScrollBar:horizontal
{
    width: 10px;
}
QScrollBar::handle
{
    background:rgba(0, 0, 0, 0.2);
    margin-left:3px;
    margin-right:3px;
    border-radius: 2px;
}
QScrollBar::handle:hover
{
    background:darkgray;
    margin-left: 2px;
    margin-right: 2px;
    border-radius: 3px;
}
QScrollBar::add-line
{
    height:14px;width:8px;
    image:url('')
}
QScrollBar::sub-line
{
    height:14px;width:8px;
    image:url('')
}
QScrollBar::add-line:hover
{
    height:14px;width:8px;
    subcontrol-position:bottom;
}
QScrollBar::sub-line:hover 
{
    height:14px;width:8px;
    subcontrol-position:top;
}
QScrollBar::add-page
{
    background:transparent;
}
QScrollBar::sub-page
{
    background:transparent; 
}
QScrollBar:hover
{
    background-color: rgba(0, 0, 0, 0.1);
}
