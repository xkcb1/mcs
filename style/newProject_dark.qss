    border-radius: 7px !important;
}
#leftDiv {
    background-color: rgb(55, 58, 60);
    border-right: 1px solid rgb(40, 43, 45);
}
#rightDiv {
    background-color: rgb(50, 53, 55);
}
#bottomDiv {
    background-color: rgb(80, 83, 85);
    border-top: 1px solid rgb(40, 43, 45);
    border-bottom-left-radius: 7px;
