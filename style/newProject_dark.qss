}
/*all*/
.QLineEdit,#lepath,.QTextEdit {
    background-color: #45494A;
    border: 2px solid #646464;
    height: 22px !important;
    border-radius: 3px;
    color: #ccc;
    font-size: 14px;
    font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
}
.QLineEdit:hover,#lepath:hover,.QTextEdit:hover {
    border: 2px solid #3c556e;
    background-color: #4e5253;
} /*#466D94;*/
.QLineEdit:focus,#lepath:focus,.QTextEdit:focus {
    border: 2px solid #466D94;
}
QCheckBox {
    color: #bbb;
}
QCheckBox::indicator  {
    background-color: rgb(100, 100, 100);
    height: 8px;
    width: 8px;
    border-radius: 7px;
    border: 3px solid rgb(122, 122, 122);
}
QCheckBox::indicator:hover {
    background-color: rgb(161, 161, 161);
    border: 3px solid #3c556e;
}
QCheckBox::indicator::checked  {
    background-color: rgb(158, 158, 158);
    border: 3px solid #4890FF;
}
QCheckBox::indicator::unchecked  {
    background-color: rgb(100, 100, 100);
    height: 8px;
    width: 8px;
    border-radius: 7px;
    border: 3px solid rgb(122, 122, 122);
}
/*all in there*/
QListView::item:selected {
   background-color: #496888;
   border: 1px solid #4890FF;
}
QListView::item:selected:active {
    background: #4B6EAF;
}
