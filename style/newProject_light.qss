/*all*/
.QLineEdit,#lepath,.QTextEdit {
    background-color: #eee;
    border: 2px solid #646464;
    height: 22px !important;
    border-radius: 3px;
    color: #111;
    font-size: 14px;
    font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;
}
.QLineEdit:hover,#lepath:hover,.QTextEdit:hover {
    border: 2px solid #3c556e;
    background-color: #eee;
} /*#466D94;*/
.QLineEdit:focus,#lepath:focus,.QTextEdit:focus {
    border: 2px solid #466D94;
}
QCheckBox {
    background-color: #eee;
    color: #222;
}
QCheckBox::indicator  {
    background-color: #ddd;
    height: 8px;
    width: 8px;
    border-radius: 7px;
    border: 3px solid rgb(122, 122, 122);
}
QCheckBox::indicator:hover {
    background-color: #eee;
    border: 3px solid #3c556e;
}
QCheckBox::indicator::checked  {
    background-color: #aaa;
    border: 3px solid #4890FF;
}
QCheckBox::indicator::unchecked  {
    background-color: #ddd;
    height: 8px;
    width: 8px;
    border-radius: 7px;
    border: 3px solid rgb(122, 122, 122);
}
/*all in there*/
QListView::item:selected {
   background-color: #e1e1e1;
   border: 1px solid #4890FF;
}
QListView::item:selected:active {
    background: #4B6EAF;
}
