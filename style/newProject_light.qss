/*make the qss(css) scrollBar*/

QComboBox:focus {
    border: 1px solid #567dbc;
    outline: none;
    
}
QComboBox:hover {
    background-color: transparent;
    outline: none;
}
QComboBox:editable {
    background: white;
    outline: none;
}
QComboBox::down-arrow {
    outline: none;
    background-color: transparent;
    image: url(./img/bottom_to.png);
}
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-left-color: darkgray;
    border-left-style: solid; /* just a single line */
    border-top-right-radius: 3px; /* same radius as the QComboBox */
    border-bottom-right-radius: 3px;
}
QComboBox QAbstractItemView {
    background-color: #22262A;
    color: #222;
    border: 1px solid #567dbc;
}
QLabel {
    color: #ccc;
}
QScrollBar 
{
    background:transparent;
    padding-top:6px;
    padding-bottom:6px;
}
/**/
QScrollBar:vertical 
{
    width: 10px;
}
QScrollBar:horizontal
{
    width: 10px;
}
QScrollBar::handle
{
    background:rgba(0, 0, 0, 0.2);
    margin-left:3px;
    margin-right:3px;
    border-radius: 2px;
}
QScrollBar::handle:hover
{
    background:darkgray;
    margin-left: 2px;
    margin-right: 2px;
    border-radius: 3px;
}
QScrollBar::add-line
{
    height:14px;width:8px;
    image:url('')
}
QScrollBar::sub-line
{
    height:14px;width:8px;
    image:url('')
}
QScrollBar::add-line:hover
{
    height:14px;width:8px;
    subcontrol-position:bottom;
}
QScrollBar::sub-line:hover 
{
    height:14px;width:8px;
    subcontrol-position:top;
}
QScrollBar::add-page
{
    background:transparent;
}
QScrollBar::sub-page
{
    background:transparent; 
}
QScrollBar:hover
{
    background-color: rgba(0, 0, 0, 0.1);
}
